<!--参考: https://github.com/k2works/sinatra_rakuten_api-->
<div class="starter-template">
  <table class="table table-hover">
    <tr>
      <th>商品名</th>
      <th>値段</th>
      <th>画像</th>
    </tr>
    <% @rankings.each do |ranking| %>
    <tr>
      <td><%= ranking['itemName'] %></td>
      <td><%= ranking['itemPrice'] %></td>
      <!--<td><a href="/sweets/"><img src="<%= ranking['smallImageUrls'][0]['imageUrl'] %>"></a></td>-->
      <td><img src="<%= ranking['mediumImageUrls'][0]['imageUrl'] %>"></td>
    </tr>
    <% end %>
  </table>
</div>
